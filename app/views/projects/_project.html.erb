<div id="<%= dom_id project %>">
  <p>
    <strong>Name:</strong>
    <%= project.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= project.description %>
  </p>

  <p>
    <strong>Begin date:</strong>
    <%= project.begin_date %>
  </p>

  <p>
    <strong>Finish date:</strong>
    <%= project.finish_date %>
  </p>

  <p>
    <strong>State:</strong>
    <%= project.state %>
  </p>

</div>
